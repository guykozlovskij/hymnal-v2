[
  {
    "number": 1,
    "title": "TRIVIENIO DIEVO PALAIMINIMAS GYRIUS JAM",
    "languages": {
      "english": 7,
      "russian": 4
    },
    "verses": [
      [
        "Tau šlovė, šlovė, o Tėve!",
        "Tau šlovė, šlovė Sūnau!",
        "Tau šlovė, šlovė o Dvasia!",
        "O šlovė, Trejybe, Tau!"
      ],
      [
        "Girkit Tėvą — Jis sumanė!",
        "O Sūnus atliko tai!",
        "Perdavė Dvasia šventoji!",
        "Trise dirba vieningai!"
      ]
    ],
    "chorus": [
      "Girkim Dievą, girkim Dievą!",
      "Jam šlovė, trys Viename!",
      "Šlovinkime mūsų Dievą! ",
      "Nuostabius Jo darbus skelbkime!"
    ]
  },
  {
    "number": 2,
    "title": "TĖVO, KAIP DVASIOS, GARBINIMAS",
    "languages": {
      "english": 11,
      "russian": 7
    },
    "verses": [
      [
        "O Tėve, Tu tiesos Dvasia.",
        "Švenčiausias iš visų;",
        "Mes Garbinam Tave dvasia",
        "Ir šaukiamės tiesoj."
      ],
      [
        "Kad garbintume mes Tave,",
        "Sukūrei dvasią mums.",
        "Todėl atsako mūs dvasia:",
        "Mes viena dvasioje."
      ],
      [
        "Per Sūnų, Tėve, atėjai.",
        "Sūnus — dabar Dvasia.",
        "Pats Dievas mūsų dvasioje —",
        "Malonė amžina."
      ],
      [
        "Sūnus tai Tavo Žodis mums.",
        "Tas Žodis tai Dvasia.",
        "Tu kaip gyvybė atėjai,",
        "Kad atgaivint mane."
      ],
      [
        "Tava Dvasia per viską ves,",
        "Kad sektume mes Juo.",
        "Taip dvasiniais mes tapsime,",
        "Ramybę rasime."
      ],
      [
        "Mes melsimės tik dvasioje",
        "Ir žvelgsim į Tave.",
        "Išlaisvint Dvasią norime,",
        "Išreikšti tik Tave."
      ],
      [
        "Tu Tėvas ir Dvasia esi,",
        "Priimki gyrių mūs.",
        "Mes dvasioje ir tiesoje",
        "Tau lenkiamės mes čia."
      ]
    ],
    "chorus": []
  },
  {
    "number": 3,
    "title": "TĖVO, KAIP MEILĖS ŠVIESOS, GARBINIMAS",
    "languages": {
      "english": 13,
      "russian": 9
    },
    "verses": [
      [
        "Viešpatie, esi Tu meilė,",
        "Viešpatie, esi šviesa.",
        "Sūnuje esi gyvybė,",
        "Dalini Save per ją."
      ],
      [
        "Tik iš meilės Tave regim,",
        "Šviesoje — ką atlikai.",
        "Vidų mūs pripildei meile,",
        "Kad mums paskui šviesą eit."
      ],
      [
        "Per malonę lieji meilę,",
        "Mums tiesa suteiks šviesos.",
        "Priartėjam mes iš meilės,",
        "Regime Tave šviesoj."
      ],
      [
        "Dievo prigimtį davei mums",
        "Tu Golgotoj, meilėje.",
        "Mūs mintis šviesa apšviečia,",
        "Krauju apsivalome."
      ],
      [
        "Pamilai, tapai gyvybe,",  
        "Kad bendrautume per ją.",
        "Šviesa Tavo veja tamsą - ",
        "Mes tada vienybėje."
      ],
      [
        "Tik per kraują ir per šviesą",
        "Patepimas mumyse.",
        "Prigimtis Tavoji — meilė",
        "Gausiai liejas į mane."
      ],
      [
        "Meilėje vaikai mes Tavo,",
        "„Abba Tėve“ — šaukiame.",
        "Ir šviesa išvis mūs tamsą,",
        "Kaip Sūnus regėsime."
      ],
      [
        "Per malonę turim šviesą,",
        "Tavo meilę turime.",
        "Ir per tai Tave pažįstam,",
        "Iš širdies dėkojame."
      ]
    ],
    "chorus": [
      "Viešpatie, meilė Tu,",
      "Viešpatie, Tu šviesa.",
      "Sūnuje esi gyvybė,",
      "Dalini Save per ją."
    ],
    "chorus-2": [
      "O, malonė. O, tiesa",
      "Štai ir meilė, ir šviesa.",
      "Tave girsim mes be galo,",
      "Ir visai pažinsime."
    ]
  }
]