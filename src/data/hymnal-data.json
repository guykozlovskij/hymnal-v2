[
  {
    "number": 1,
    "title": "TRIVIENIO DIEVO PALAIMINIMAS",
    "sub-title": "GYRIUS JAM",
    "languages": {
      "russian": 4,
      "english": 7
    },
    "hymnal.net-url": "https://www.hymnal.net/en/hymn/h/7",
    "melody-url": "https://www.hymnal.net/Hymns/Hymnal/mp3/e0007_full.mp3",
    "verses": [
      [
        "Tau šlovė, šlovė, o Tėve!",
        "Tau šlovė, šlovė Sūnau!",
        "Tau šlovė, šlovė o Dvasia!",
        "O šlovė, Trejybe, Tau!"
      ],
      [
        "Girkit Tėvą — Jis sumanė!",
        "O Sūnus atliko tai!",
        "Perdavė Dvasia šventoji!",
        "Trise dirba vieningai!"
      ]
    ],
    "chorus": [
      "Girkim Dievą, girkim Dievą!",
      "Jam šlovė, trys Viename!",
      "Šlovinkime mūsų Dievą! ",
      "Nuostabius Jo darbus skelbkime!"
    ]
  },
  {
    "number": 2,
    "title": "TĖVO, KAIP DVASIOS, GARBINIMAS",
    "languages": {
      "russian": 7,
      "english": 11
    },
    "hymnal.net-url": "https://www.hymnal.net/en/hymn/h/11",
    "melody-url": "https://www.hymnal.net/Hymns/Hymnal/mp3/e0011_full.mp3",
    "verses": [
      [
        "O Tėve, Tu tiesos Dvasia.",
        "Švenčiausias iš visų;",
        "Mes Garbinam Tave dvasia",
        "Ir šaukiamės tiesoj."
      ],
      [
        "Kad garbintume mes Tave,",
        "Sukūrei dvasią mums.",
        "Todėl atsako mūs dvasia:",
        "Mes viena dvasioje."
      ],
      [
        "Per Sūnų, Tėve, atėjai.",
        "Sūnus — dabar Dvasia.",
        "Pats Dievas mūsų dvasioje —",
        "Malonė amžina."
      ],
      [
        "Sūnus tai Tavo Žodis mums.",
        "Tas Žodis tai Dvasia.",
        "Tu kaip gyvybė atėjai,",
        "Kad atgaivint mane."
      ],
      [
        "Tava Dvasia per viską ves,",
        "Kad sektume mes Juo.",
        "Taip dvasiniais mes tapsime,",
        "Ramybę rasime."
      ],
      [
        "Mes melsimės tik dvasioje",
        "Ir žvelgsim į Tave.",
        "Išlaisvint Dvasią norime,",
        "Išreikšti tik Tave."
      ],
      [
        "Tu Tėvas ir Dvasia esi,",
        "Priimki gyrių mūs.",
        "Mes dvasioje ir tiesoje",
        "Tau lenkiamės mes čia."
      ]
    ]
  },
  {
    "number": 3,
    "title": "TĖVO, KAIP MEILĖS ŠVIESOS, GARBINIMAS",
    "languages": {
      "russian": 9,
      "english": 13
    },
    "hymnal.net-url": "https://www.hymnal.net/en/hymn/h/13",
    "melody-url": "https://www.hymnal.net/Hymns/Hymnal/mp3/e0013_full.mp3",
    "verses": [
      [
        "Viešpatie, esi Tu meilė,",
        "Viešpatie, esi šviesa.",
        "Sūnuje esi gyvybė,",
        "Dalini Save per ją."
      ],
      [
        "Tik iš meilės Tave regim,",
        "Šviesoje — ką atlikai.",
        "Vidų mūs pripildei meile,",
        "Kad mums paskui šviesą eit."
      ],
      [
        "Per malonę lieji meilę,",
        "Mums tiesa suteiks šviesos.",
        "Priartėjam mes iš meilės,",
        "Regime Tave šviesoj."
      ],
      [
        "Dievo prigimtį davei mums",
        "Tu Golgotoj, meilėje.",
        "Mūs mintis šviesa apšviečia,",
        "Krauju apsivalome."
      ],
      [
        "Pamilai, tapai gyvybe,",
        "Kad bendrautume per ją.",
        "Šviesa Tavo veja tamsą - ",
        "Mes tada vienybėje."
      ],
      [
        "Tik per kraują ir per šviesą",
        "Patepimas mumyse.",
        "Prigimtis Tavoji — meilė",
        "Gausiai liejas į mane."
      ],
      [
        "Meilėje vaikai mes Tavo,",
        "„Abba Tėve“ — šaukiame.",
        "Ir šviesa išvis mūs tamsą,",
        "Kaip Sūnus regėsime."
      ],
      [
        "Per malonę turim šviesą,",
        "Tavo meilę turime.",
        "Ir per tai Tave pažįstam,",
        "Iš širdies dėkojame."
      ]
    ],
    "chorus": [
      "Viešpatie, meilė Tu,",
      "Viešpatie, Tu šviesa.",
      "Sūnuje esi gyvybė,",
      "Dalini Save per ją."
    ],
    "chorus-2": [
      "O, malonė. O, tiesa",
      "Štai ir meilė, ir šviesa.",
      "Tave girsim mes be galo,",
      "Ir visai pažinsime."
    ]
  },
  {
    "number": 4,
    "title": "TĖVO GARBINIMAS UŽ JO NEMIRTINGUMĄ",
    "sub-title": "",
    "languages": {
      "russian": 10,
      "english": 14
    },
    "hymnal.net-url": "https://www.hymnal.net/en/hymn/h/14",
    "melody-url": "https://www.hymnal.net/Hymns/Hymnal/mp3/e0014_full.mp3",
    "verses": [
      [
        "Esi nemirtingas, nematomas Tu.",
        "Šlovė Tavo dengia Tave nuo žmonių.",
        "Didingas, Tu Dieve, Tavy dienos mūs.",
        "Mes garbinsim Tave, o Tėve brangus."
      ],
      [
        "Valdai Tu visatą Savąja galia,",
        "Ir nieks apriboti negali Tavęs.",
        "Teisus Tavo teismas kaip Tavo kalnai,",
        "Kaip lietūs dangaus Tavo meilės srautai."
      ],
      [
        "Kas buvo mirty, tam gyvybę teiki.",
        "Esi visame, Tu mums viskas esi.",
        "Mes žydim ir vystam kaip lauko gėlė,",
        "O Dieve, Tavy nė dalelės mirties."
      ],
      [
        "O Dieve, koksai Tu Didingas esi,",
        "Kad net angelai veidus dengia visi.",
        "Tu Tėve mūs Didis, šlovės ir šviesos.",
        "Visatos Kūrėjas — Tavy nėr tamsos."
      ],
      [
        "Esi nemirtingas, nematomas Tu.",
        "Šlovė Tavo dengia Tave nuo žmonių.",
        "Didingas Tu, Dieve, Tavy dienos mūs.",
        "Mes garbinsim Tave, o Tėve brangus."
      ]
    ],
    "chorus": []
  },
  {
    "number": 5,
    "title": "TĖVO GARBINIMAS UŽ JO ŠVENTUMĄ",
    "sub-title": "",
    "languages": {
      "russian": 18,
      "english": 22
    },
    "hymnal.net-url": "https://www.hymnal.net/en/hymn/h/22",
    "melody-url": "https://www.hymnal.net/Hymns/Hymnal/mp3/e0022_full.mp3",
    "verses": [
      [
        "Šventas Tėve Dangiškasis,",
        "Tavo vardą aukštinam,",
        "Mes Šventu Tave vadinam,",
        "Džiaugiamės ir garbinam."
      ],
      [
        "Tu teisus per amžius, Tėve.",
        "Myli Tavo širdis mus.",
        "Tavo prigimtį — šventumą",
        "Kristus perduoda į mus."
      ],
      [
        "Jėzaus Viešpaties krauju Tu",
        "Amžiams mus pašventinai.",
        "Ta tiesa, kur Tavo Žody,",
        "Mus vergus išlaisvinai."
      ],
      [
        "Taip šventaisiais padarei mus",
        "Tu sava Šventa Dvasia.",
        "Tiktai Ja tegu šventėja",
        "Kūnas, siela ir dvasia."
      ],
      [
        "Jėzaus šventąją gyvybę",
        "Per malonę gavome.",
        "Ir visi šventumą Tavo",
        "Mes dalinti galime."
      ],
      [
        "Ir kada Šventam mieste mes",
        "Būsim viena šventume.",
        "Iki žemės pakraščių net",
        "„Šventas Tu“ giedosime."
      ]
    ],
    "chorus": []
  },
  {
    "number": 6,
    "title": "TĖVO GARBINIMAS UŽ JO NAUJUMĄ",
    "sub-title": "",
    "languages": {
      "russian": 12,
      "english": 16
    },
    "hymnal.net-url": "https://www.hymnal.net/en/hymn/h/16",
    "melody-url": "https://www.hymnal.net/Hymns/Hymnal/mp3/e0016_full.mp3",
    "verses": [
      [
        "Neblėstantis, Tu Tėvas mūs",
        "Naujumas Tavyje.",
        "Tu kaip rasa gaivinantis.",
        "Pagirdai Tu mane."
      ],
      [
        "Tave pažįstame visi",
        "Ir vėl, ir vėl naujai.",
        "Tu atgaivą visiems teiki.",
        "Nors metai bėga greit."
      ],
      [
        "Palaiminimus suteiki",
        "Ir vėl, ir vėl naujus",
        "Naujoji Sandora Tava",
        "Atvėrė kelią mums."
      ],
      [
        "Tu naują širdį mums davei",
        "Ir dvasią atgimdei.",
        "Dabar mes esame visi",
        "Naujajam Kūriny."
      ],
      [
        "Atnaujini Tu, Tėve, mus —",
        "Kas sena naikini.",
        "Kiekvieną dieną naują Tu",
        "Gyvybę suteiki."
      ],
      [
        "Dangus ir žemė bus nauji.",
        "Ir viskas nauja bus",
        "Gyvybės medis duos vaisius",
        "Per ištisus metus."
      ],
      [
        "Naujumas Tavo amžinas.",
        "Tu Naujas visame.",
        "Nauja giesme mes girsime",
        "Per amžius tik Tave."
      ]
    ],
    "chorus": [
      "Tu nesikeičiantis Tėvas,",
      "Tu jaunas amžinai.",
      "Per amžius atskleidi Save",
      "Naujovėj nuostabioj."
    ]
  },
  {
    "number": 7,
    "title": "TĖVO GARBINIMAS JO DIDYBĖ",
    "sub-title": "",
    "languages": {
      "russian": 13,
      "english": 17
    },
    "hymnal.net-url": "https://www.hymnal.net/en/hymn/h/17",
    "melody-url": "https://www.hymnal.net/Hymns/Hymnal/mp3/e0017_full.mp3",
    "verses": [
      [
        "O Tėve, kai žiūriu į kūriniją.",
        "Į dangų, žemę ir jų stebuklus.",
        "Suvokti mums gebėjimo pristinga —",
        "Tu taip apreiški Savo jėgą mums."
      ],
      [
        "Priimdamas malonę Tavo, Tėve.",
        "Galvoju apie Tavo Sūnų aš.",
        "Jis mirė, kad Naujasis Kūrinys Jo",
        "Gyvybę teiktų Jo pilnatvėje."
      ],
      [
        "Susirinkime, Jo malonėj būnant,",
        "Matau, kad gyvas daugelyje Tu.",
        "Ir šitaip ugdoma Bažnyčia Tavo.",
        "Ir iš vidaus per mus reiškiesi Tu."
      ],
      [
        "Nauja Jeruzale visi mes būsim.",
        "Pilnatvės amžiaus laukiam su džiaugsmu.",
        "Ir su naujaisiais dangumi ir žeme —",
        "Mes tapsime Tavu išreiškimu."
      ]
    ],
    "chorus": [
      "Mana dvasia štai gieda, Tėve, Tau.",
      "Koks Didis Tu, koks nuostabus!",
      "Ir amžinai giedosim giesmę šią:",
      "Koks Didis Tu, koks nuostabus."
    ]
  },
  {
    "number": 8,
    "title": "KAS APSAKYS DIDYBĘ JO",
    "sub-title": "",
    "hymnal.net-url": "",
    "melody-url": "",
    "verses": [
      [
        "Kas apsakys didybę Jo ir valdžią pamatuos,",
        "Jis trykšta gyvybės jėga!",
        "Priklaups karaliai žemės šios ir Jėzaus",
        "vardui nusilenks, šaltiniui Meilės ir Šviesos! }2k",
        "Kas apsakys didybę Jo.",
        "Kas apsakys Meilę Kristaus Viešpaties?",
        "Kas apsakys didybę Jo?",
        "Kas apsakys šviesą Jo?",
        "Kas apsakys meilę Kristaus?"
      ]
    ]
  },
  {
    "number": 9,
    "title": "TĖVO GARBINIMAS JO MEILĖ",
    "sub-title": "",
    "languages": {
      "russian": 26,
      "english": 30
    },
    "hymnal.net-url": "https://www.hymnal.net/en/hymn/h/30",
    "melody-url": "https://www.hymnal.net/Hymns/Hymnal/mp3/e0030_full.mp3",
    "verses": [
      [
        "O, kokią meilę mums davei!",
        "Dėkojam Tau išties.",
        "Tau, Tėve, lenkiamės žemai,",
        "Dėkojam iš širdies."
      ],
      [
        "Atvėrei Savo tikslą mums.",
        "Sumanymą širdies.",
        "Save apreiškei Sūnuje.",
        "Tu mus vedi išties."
      ],
      [
        "Tu brangų Sūnų mums davei —",
        "Jis numirė už mus,",
        "Kad taptume mes sūnumis,",
        "Paveldėję turtus."
      ],
      [
        "Gyvybę turime Jame.",
        "Tu Tėvu mūs tapai.",
        "Esybę Savo ir Save",
        "Tu mums atidavei."
      ],
      [
        "Tava Dvasia mūs dvasioje.",
        "Tau „Abba“ šaukiame.",
        "Mes gimę esame Jame.",
        "Į Jį mes keičiamės."
      ],
      [
        "Šlovėn atvesti sūnus Tu —",
        "To troškai visada.",
        "Mes tapsim atvaizdu Sūnaus",
        "Duota Tava jėga."
      ],
      [
        "Tu atmainysi mus visus —",
        "Kiekvienas tau brangus.",
        "Atvesi Tu į šlovę mus",
        "Mes tikim iš tiesų."
      ],
      [
        "O, kokią meilę suteikei!",
        "Mes Tau dėkojame!",
        "Per amžius šlovinsim Tave",
        "Savose širdyse."
      ]
    ],
    "chorus": []
  },
  {
    "number": 10,
    "title": "TĖVO GARBINIMAS UŽ JO ATPIRKIMĄ",
    "sub-title": "",
    "languages": {
      "russian": 31,
      "english": 39
    },
    "hymnal.net-url": "https://www.hymnal.net/en/hymn/h/39",
    "melody-url": "https://www.hymnal.net/Hymns/Hymnal/mp3/e0039_full.mp3",
    "verses": [
      [
        "Kas gali daugiau atiduot negu Jis:",
        "Dėl mūsų atidavė Sūnų numirt.",
        "Krauju Jis Savuoju nuplovė visus,",
        "Gyvenimo tikro atkėlė vartus."
      ],
      [
        "Atpirko visus mus brangiuoju krauju.",
        "„Tikėk“ — Viešpats sako Savuoju Žodžiu.",
        "Nors tu nuodėmėj, nebėra tau vilties,",
        "Jo šaukis, tada nebeliks jau kaltės."
      ],
      [
        "Kas gali daugiau atiduot negu Jis:",
        "Per Sūnų palaimą mes turim visi.",
        "Bet džiaugsmas tada neturės jau ribų,",
        "Kada susitiksime su Viešpačiu."
      ]
    ],
    "chorus": [
      "Tau šlovė! Tau šlovė! Te išgirsta Tave!",
      "Tau šlovė! Tau šlovė! Džiaugias žemė visa!",
      "Ateikit per Sūnų, šlovė Tėvui Jo!",
      "Nuostabūs darbai, ką atliko Jisai!"
    ]
  }
]