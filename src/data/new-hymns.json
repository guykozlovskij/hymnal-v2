[
  {
    "number": 1,
    "title": "RAGINIMAS BENDRAUTI SU VIEŠPAČIU",
    "languages": {
      "russian": 463,
      "english": 643
    },
    "hymnal.net-url": "https://www.hymnal.net/en/hymn/nt/643",
    "melody-url": "https://www.hymnal.net/Hymns/NewTunes/mp3/e0643_new.mp3",
    "verses": [
      [
        "Skirk laiko regėt Jį,",
        "Kalbėki su Juo,",
        "Žodžiu Jo maitinkis,",
        "Jame pasilik.",
        "Lauk Jo buvime,",
        "Romus, nuolankus,",
        "Ir tik Jo palaiminimo",
        "Tu ieškok."
      ],
      [
        "Skirk laiko regėt Jį,",
        "Pasaulis skubus.",
        "Daug laiko su Jėzum",
        "Praleiski slaptoj.",
        "Žvelgdamas į Jėzų,",
        "Tu būsi kaip Jis.",
        "Tavam elgesy",
        "Jo panašumą matys."
      ],
      [
        "Skirk laiko regėt Jį,",
        "Leisk būt Jam vedliu",
        "Ir kas benutiktų,",
        "Nebėk prieky Jo.",
        "Džiaugsme,liūdesy",
        "Vis dar seki Jį,",
        "Žiūrėki į Jėzų,",
        "Tikėk Jo žodžiu."
      ],
      [
        "Skirk laiko regėt Jį,",
        "Būk sieloj ramus.",
        "Mintis ir jausmus Jam",
        "Paveski savus.",
        "Prie meilės šaltinių",
        "Dvasia Jo nuves,",
        "Tuomet Jo malonė",
        "Tau bus suteikta."
      ]
    ]
  },
  {
    "number": 2,
    "title": "VIEŠPATIES ŠLOVINIMAS UŽ JO IŠGELBĖJIMĄ",
    "languages": {
      "russian": 84,
      "english": 116
    },
    "hymnal.net-url": "https://www.hymnal.net/en/hymn/h/116",
    "melody-url": "https://www.hymnal.net/Hymns/Hymnal/mp3/e0116_full.mp3",
    "verses": [
      [
        "Koks atpirkimas nuostabus",
        "O, Viešpatie, Tavy,",
        "Per amžius neregėta tai,",
        "Ką Tu man padarei.",
        "Tu paslaptingas Dieviškas",
        "Nėr žodžių apsakyt.",
        "Šis atpirkimas nuostabus",
        "Aukščiau už gįrius mūs."
      ],
      [
        "Už mus ant kryžiaus perdurtas, -",
        "Ten kraujas ir vanduo, -",
        "Tekėjo mums suteikti jie givybę Dievišką.",
        "Nuplovė ir atpirko mus",
        "Mums būt priimtinais.",
        "Atnaujinti givybėje",
        "Mes esam Tavyje."
      ],
      [
        "Esi Tu grūdas Dieviškas,",
        "Mirei, kad daug grūdų",
        "Į kūną sumaišyti juos,",
        "Dalinti prigimtį.",
        "Mes Tavo padidėjimas,",
        "Tu mūsų sudėtis.",
        "Per mus judi ir gyveni",
        "Išreikštas Tu esi."
      ],
      [
        "Mes Tavo Kūnas",
        "Įsikurt gali Tu mumyse.",
        "Įgytį Sau namus,",
        "Atrast pasitikėjimą.",
        "Mes esam Tavo kopija -",
        "Patenkintas Esi",
        "Viskuom kas Tu esi Kūne",
        "Mėgaujames visi."
      ],
      [
        "Žiūrėdami į ženklus čia",
        "Prisimenam Tave.",
        "Už dydį aptirkimą",
        "Šlovę atiduodame.",
        "Mes - tavo padidėjimas,",
        "Namai ir nuotaka.",
        "Dėkojam, šlovinam Tave",
        "Ir aukštinsim visad."
      ]
    ]
  },
  {
    "number": 3,
    "title": "MIELIAUSIAS VIEŠPATIE, TU PAŠAUKEI MUS ČIA",
    "hymnal.net-url": "https://www.hymnal.net/en/hymn/ns/97",
    "melody-url": "https://www.hymnal.net/Hymns/NewSongs/mp3/ns0097.mp3",
    "verses": [
      [
        "Viešpatie, pašaukes mus,",
        "atvėrei širdį Tu.",
        "Mums įeit džiugu į Arką,",
        "Kai girdime Tave.",
        "Tavo kelias gelbėt mus",
        "Diena iš dienos.",
        "Pasilikę Tavyje,",
        "Virš pikto amžiaus esame."
      ],
      [
        "Siekiame Šio amžiaus skonio,",
        "Ir dalies jame.",
        "Jei tiktai Žinotume,",
        "kaip liūdinam Tave.",
        "Atgailaujančia Širdim ateitume tada,",
        "Atiduotume save,",
        "Su Tavim būti Arkoje."
      ],
      [
        "Nors ir liejas išorėje vandenys mirties,",
        "Tavyje mes saugūs, Viešpatie!",
        "Tik Tau šlovė!",
        "Tai ka as myleiau",
        "Vandenyno jau dugne.",
        "Išlaikyk mus atvirus,",
        "Mes Arkoje, bažnyčioje."
      ]
    ],
    "chorus": [
      "Tavo akyse malonę randam mes,",
      "Atvirom Širdim ateiname.",
      "Ka sako mums draugai nerūpi jau daugiau,",
      "Jėzau, mylim mes Tave."
    ]
  },
  {
    "number": 4,
    "title": "KRISTAUS KAIP GYVYBĖS PATYRIMAS",
    "languages": {
      "russian": 355
    },
    "hymnal.net-url": "https://www.hymnal.net/en/hymn/ns/138",
    "melody-url": "https://www.hymnal.net/Hymns/NewSongs/mp3/ns0138.mp3",
    "verses": [
      [
        "Tai, ką pasiekt norejau,",
        "Prarasta tapo man.",
        "Jėzus išlaisvino",
        "Nuo to, kuo buvau!",
        "Viską, ką aš turėjau,",
        "Blogo ar gero,",
        "Šiukslėmis tai laikau,",
        "Kad įgyčiau Jį!"
      ],
      [
        "Pamirški, kas praėjo,",
        "Į priekį veržkis tu!",
        "Sieki Šitų dalykų",
        "Su šia mintim.",
        "Visad norėk daugiau!",
        "Veržkis užaugt į Jį,",
        "Kristų pažink,",
        "Kad būtum rastas Jame."
      ]
    ],
    "chorus": [
      "Nepasiduok!",
      "Bėgsiu, kol nepasieksiu",
      "Tikslo, kuriam",
      "Esu aš pašauktas!",
      "Eisiu pirmyn!",
      "Veršiuosi ir laikysiuos",
      "Aš tiktai To, Kurs laiko mane!"
    ]
  },
  {
    "number": 5,
    "title": "KRISTAUS KAIP GYVYBĖS PATYRIMAS",
    "languages": {
      "russian": 505,
      "english": 355
    },
    "hymnal.net-url": "https://www.hymnal.net/en/hymn/h/505",
    "melody-url": "https://www.hymnal.net/Hymns/Hymnal/mp3/e0505_full.mp3",
    "verses": [
      [
        "Štai Žmogus šlovėje,",
        "Gyvybė Jo man.",
        "Jis tyras ir šventas,",
        "Triumfuojantis Jis.",
        "Jis laisvas ir mylintis,",
        "Koks Jis švelnus!",
        "Turėti gyvybę",
        "Šlovėj Jo galiu."
      ],
      [
        "Štai Žmogus šlovėje,",
        "Gyvybė Jo man.",
        "Šėtoną įveikęs",
        "Be pančių Jisai",
        "Gyvybėje valdo,",
        "Koksai Jis puikus!",
        "Turėti gyvybę",
        "Šlovej Jo galiu."
      ],
      [
        "Štai Žmogus šlovėje,",
        "Gyvybė Jo man.",
        "Jame nėr silpnybių,",
        "Ligų nėr Jame.",
        "Stiprus ir toks tvirtas",
        "Džiaugsmingas Jisai!",
        "Turėsiu gyvybę",
        "Aš Jo šlovėj."
      ],
      [
        "Štai Žmogus šlovėje,",
        "Gyvybė Jo man.",
        "Ramybė tvirta Jo,",
        "Koksai Jis kantrus!",
        "Spindėdamas laukia",
        "Išvyst manyje",
        "Kaip Jo gyvybe",
        "Many bus atskleista.",
        "",
        "(Paskutines dvi eilutės kartojamos du kartus)"
      ]
    ]
  },
  {
    "number": 6,
    "title": "EVANGELIJOS TIKSLAS",
    "hymnal.net-url": "https://www.hymnal.net/en/hymn/ns/361",
    "melody-url": "https://www.hymnal.net/Hymns/NewSongs/mp3/ns0361.mp3",
    "verses": [
      [
        "Evangelijos tikslas -",
        "Kad mylėdami Jėzų",
        "pirma meile Jam,",
        "Mes išlietume viską,",
        "tai, kas, brangiausia,",
        "Ant Jo išlietume mes;",
        "Vertingus dvasinius turtus,",
        "viska, ka turim,",
        "Save, \"išsvaistytume\" Jam,",
        "Vertingus dvasinius turtus,",
        "viska, ka turim,",
        "Save, \"išsvaistytume\" Jam."
      ]
    ]
  },
  {
    "number": 7,
    "title": "JEI MES TURIM ŠVELNIĄ MEILĘ JAM",
    "hymnal.net-url": "https://www.hymnal.net/en/hymn/ns/618",
    "melody-url": "https://www.hymnal.net/Hymns/NewSongs/mp3/ns0618.mp3",
    "verses": [
      [
        "Jei mes turim švelnią meilę Jam,",
        "Švelnia meilę Viešpačiui Jėzui,",
        "Mūs liežuvis bus kaip plunksna",
        "Pasiruošusio rašytojo,",
        "Rašyti mūsų meilę Jam,",
        "Mūsų gyrių, gyrių, Jam",
        "Mūs patyrimais ir pasimėgavimais",
        "Pagal viską, pagal viską,",
        "Pagal viską, kas Jis mums yra."
      ]
    ]
  },
  {
    "number": 8,
    "title": "NAZARĖNŲ DAINA",
    "hymnal.net-url": "https://www.hymnal.net/en/hymn/ns/315",
    "melody-url": "https://www.hymnal.net/Hymns/NewSongs/mp3/ns0315.mp3",
    "verses": [
      [
        "Šiame amžiuje",
        "Reikia Samuelių Tau;",
        "Tų, kas turi aiškią naštą ekonomikos.",
        "" ,
        "Tavo arka kur?",
        "Ir tie, kam rūpėtum Tu,",
        "Kad Savos Širdies troškimą Tu igytumei?",
        "" ,
        "Nazariečius Tavo poreikiams pakelk,",
        "Pranašus, teisėjus, kunigus,",
        "Tavo Žodį kalbèt su Tavo valdžia",
        "ir būti atvaizdu Tavos širdies."
      ],
      [
        "Viešpačiui judėt",
        "Reikia irankį įgyt-",
        "Nugalėtojus, atskirtus Jam nuo amžiaus Šio.",
        "",
        "Nugalėtojai sustatyti Jo ŽodŽiu,",
        "Prisijungę jie per žodį",
        "Prie troškimo Jo.",
        "",
        "Nazariečius Tavo poreikiams pakelk,",
        "Savo noru pasišventusius,",
        "Prisijungusius prie širdies Tavos",
        "Gyvam liudijimui žemėje."
      ],
      [
        "Dievui reik žmoniy,",
        "Kurie tiesia kelią Jam;",
        "Ekonomika įvykdoma Per jų maldas.",
        "",
        "Bijo jie įžeist",
        "Dievą ir buvimą Jo,",
        "Bet nebijo nemylėt savęs, pasiaukot.",
        "",
        "Nazariečiais padaryk mus, Viešpatie,",
        "Tais, kurie užbaigtų amžių šį,",
        "Tie, kas viena su Tavim karalystei įvest-",
        "Jaunimą, pasišventusį tik Tau."
      ]
    ]
  }
]